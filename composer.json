{
  "name": "webwingscz/oauth2-server",
  "description": "An integration of League\\OAuth2Server into Nette framework",
  "keywords": [
    "nette",
    "contributte",
    "php",
    "oauth2",
    "oauth2 server"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "league/oauth2-server": "^8.4 || ^9.1",
    "nette/di": "^3.0.14"
  },
  "require-dev": {
    "contributte/psr7-http-message": "^0.9",
    "nette/application": "^3.1.8"
  },
  "conflict": {
    "nette/utils": "<3.2.5"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\OAuth2Server\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
